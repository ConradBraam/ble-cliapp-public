{
    "macros": [
        "MEM_ALLOC=malloc",
        "MEM_FREE=free",
        "NDEBUG=1",
        "OS_TASKCNT=1",
        "OS_IDLESTKSIZE=32",
        "OS_STKSIZE=1",
        "OS_TIMERS=0",
        "OS_FIFOSZ=4",
        "OS_MUTEXCNT=1"
    ],
    "target_overrides": {
        "*": {
            "target.features_add": ["COMMON_PAL"],
            "mbed-trace.enable": 1,
	    "core.stdio-flush-at-exit": false
        }
    }
}
